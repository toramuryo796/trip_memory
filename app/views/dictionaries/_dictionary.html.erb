<tr class="dictionary-list" id="dictionary-list-<%= dictionary.id %>">
  <td class="japanese dictionary-content">
    <%= dictionary.japanese %>
  </td>

  <td class="translation dictionary-content">
    <%= dictionary.translation %>
  </td>
 
  <td class="dictionary-update">
    <%= link_to "編集", edit_group_plan_dictionary_path(@group, @plan, dictionary), data:{remote: true} %>
  </td>

  <td class="dictionary-destroy">
    <%= link_to "削除", group_plan_dictionary_path(@group, @plan, dictionary), 
      method: :delete,
      data:{remote: true} %>
  </td>
</tr>